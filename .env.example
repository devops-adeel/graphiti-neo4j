# Neo4j Credentials
# IMPORTANT: Copy this file to .env and update with your actual values
NEO4J_USER=neo4j
NEO4J_PASSWORD=your-secure-password-here

# Connection URIs for different access methods
NEO4J_URI_ORBSTACK=bolt://neo4j.graphiti.local:7687
NEO4J_URI_DOCKER=bolt://neo4j-graphiti:7687
NEO4J_URI_LOCAL=bolt://localhost:7687

# Browser Access URLs
NEO4J_BROWSER_LOCAL=http://localhost:7474
NEO4J_BROWSER_ORBSTACK=http://neo4j.graphiti.local:7474